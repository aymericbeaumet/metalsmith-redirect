{
  "name": "metalsmith-redirect",
  "version": "2.1.0",
  "description": "A Metalsmith plugin to create HTTP redirections",
  "author": {
    "name": "Aymeric Beaumet",
    "email": "hi@aymericbeaumet.com",
    "url": "aymericbeaumet.com"
  },
  "license": "CC0-1.0",
  "repository": "aymericbeaumet/metalsmith-redirect",
  "keywords": [
    "metalsmith",
    "plugin",
    "http",
    "https",
    "redirect",
    "redirection"
  ],
  "main": "dist",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "ava": "^0.15.0",
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.3",
    "gulp": "^3.9.1",
    "gulpsmith": "^0.5.5",
    "metalsmith": "^1.7.0",
    "nyc": "^7.1.0",
    "rimraf": "^2.5.4",
    "standard": "^7.1.2"
  },
  "scripts": {
    "test": "standard && npm run prepublish && nyc --reporter=html ava",
    "prepublish": "babel lib --out-dir=dist",
    "codeclimate": "nyc report --reporter=text-lcov | codeclimate-test-reporter"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ],
    "presets": [
      "es2015"
    ],
    "sourceMaps": "inline"
  }
}
