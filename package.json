{
  "name": "metalsmith-redirect",
  "version": "3.0.2",
  "description": "A Metalsmith plugin to create HTTP redirections",
  "keywords": [
    "metalsmith",
    "metalsmith-plugin",
    "plugin",
    "redirect"
  ],
  "homepage": "https://github.com/aymericbeaumet/metalsmith-redirect",
  "repository": {
    "type": "git",
    "url": "https://github.com/aymericbeaumet/metalsmith-redirect.git"
  },
  "license": "MIT",
  "author": {
    "name": "Aymeric Beaumet",
    "email": "hi@aymericbeaumet.com",
    "url": "https://aymericbeaumet.com"
  },
  "files": [
    "index.js",
    "lib/*"
  ],
  "scripts": {
    "format": "prettier --write \"{,.}**.{js,md,yml}\" && xo --fix",
    "test": "prettier --check \"{,.}**.{js,md,yml}\" && xo && ava"
  },
  "xo": {
    "prettier": true
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "ava": "2.3.0",
    "gulp": "4.0.2",
    "gulpsmith": "0.6.0",
    "htmlparser2": "4.0.0",
    "metalsmith": "2.3.0",
    "metalsmith-broken-link-checker": "1.0.2",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "xo": "0.24.0"
  }
}
